{
    "AllowedHosts": "*",
    "Cors": {
        "Name": "WebClientCorsPolicy",
        "AllowedOrigins": "https://localhost:5002",
        "AllowedMethods": "GET"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    }
}
